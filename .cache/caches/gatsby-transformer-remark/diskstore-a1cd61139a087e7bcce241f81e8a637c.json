 len then narrow down the prefix.","position":{"start":{"line":33,"column":1,"offset":582},"end":{"line":33,"column":75,"offset":656}}}],"position":{"start":{"line":33,"column":1,"offset":582},"end":{"line":33,"column":75,"offset":656}}},{"type":"text","value":"\n"},{"type":"element","tagName":"pre","properties":{},"children":[{"type":"element","tagName":"code","properties":{"className":["language-js"]},"children":[{"type":"text","value":"/**\n * @param {string[]} strs\n * @return {string}\n */\nlet longestCommonPrefix = function (strs) {\n    if (strs.length > 0) {\n        let minLen = Math.min(...strs.map((s) => s.length));\n        const anyStr = strs[0];\n        while (minLen) {\n            const prefix = anyStr.slice(0, minLen--);\n            if (strs.every((s) => s.startsWith(prefix))) {\n                return prefix;\n            }\n        }\n    }\n    return '';\n};\n"}],"position":{"start":{"line":35,"column":1,"offset":660},"end":{"line":53,"column":4,"offset":1122}}}],"position":{"start":{"line":35,"column":1,"offset":660},"end":{"line":53,"column":4,"offset":1122}}},{"type":"text","value":"\n"},{"type":"element","tagName":"h4","properties":{},"children":[{"type":"text","value":"TWO","position":{"start":{"line":55,"column":6,"offset":1131},"end":{"line":55,"column":9,"offset":1134}}}],"position":{"start":{"line":55,"column":1,"offset":1126},"end":{"line":55,"column":9,"offset":1134}}},{"type":"text","value":"\n"},{"type":"element","tagName":"pre","properties":{},"children":[{"type":"element","tagName":"code","properties":{"className":["language-js"]},"children":[{"type":"text","value":"/**\n * @param {string[]} strs\n * @return {string}\n */\nlet longestCommonPrefix = function (strs) {\n    if (strs.length <= 0) {\n        return '';\n    }\n\n    let i = 0;\n    while (strs.every((s) => s[i] && s[i] === strs[0][i])) {\n        i++;\n    }\n    return strs[0].slice(0, i);\n};\n"}],"position":{"start":{"line":57,"column":1,"offset":1138},"end":{"lin