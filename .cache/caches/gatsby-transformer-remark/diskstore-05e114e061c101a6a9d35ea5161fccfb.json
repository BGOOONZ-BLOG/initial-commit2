:[{"type":"text","value":"let fruits = ['apple', 'banana', 'grapes', 'mango', 'orange']\n\n/**\n * Filter array items based on search criteria (query)\n */\nfunction filterItems(arr, query) {\n  return arr.filter(function(el) {\n      return el.toLowerCase().indexOf(query.toLowerCase()) !== -1\n  })\n}\n\nconsole.log(filterItems(fruits, 'ap'))  // ['apple', 'grapes']\nconsole.log(filterItems(fruits, 'an'))  // ['banana', 'mango', 'orange']\n"}],"position":{"start":{"line":176,"column":1,"offset":6327},"end":{"line":188,"column":77,"offset":6788}}}],"position":{"start":{"line":176,"column":1,"offset":6327},"end":{"line":188,"column":77,"offset":6788}}},{"type":"text","value":"\n"},{"type":"element","tagName":"h4","properties":{},"children":[{"type":"text","value":"ES2015 Implementation","position":{"start":{"line":190,"column":6,"offset":6797},"end":{"line":190,"column":27,"offset":6818}}}],"position":{"start":{"line":190,"column":1,"offset":6792},"end":{"line":190,"column":27,"offset":6818}}},{"type":"text","value":"\n"},{"type":"element","tagName":"pre","properties":{},"children":[{"type":"element","tagName":"code","properties":{},"children":[{"type":"text","value":"const fruits = ['apple', 'banana', 'grapes', 'mango', 'orange']\n\n/**\n * Filter array items based on search criteria (query)\n */\nconst filterItems = (arr, query) => {\n  return arr.filter(el => el.toLowerCase().indexOf(query.toLowerCase()) !== -1)\n}\n\nconsole.log(filterItems(fruits, 'ap'))  // ['apple', 'grapes']\nconsole.log(filterItems(fruits, 'an'))  // ['banana', 'mango', 'orange']\n"}],"position":{"start":{"line":192,"column":1,"offset":6822},"end":{"line":202,"column":77,"offset":7252}}}],"position":{"start":{"line":192,"column":1,"offset":6822},"end":{"line":202,"column":77,"offset":7252}}},{"type":"text","value":"\n"},{"type":"element","tagName":"h3","properties":{},"children":[{"type":"text","value":"Affecting Initial Array (modifying, appending and deleting)","position":{"start":{"line":